{
  "expo": {
    "name": "Elysia",
    "slug": "Elysia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0cb012"
    },
    "extra": {
      "appName": "Elysia Mobile",
      "appVersion": "1.0.0",
      "buildNumber": "1",
      "gitCommit": "e2c9a12"  
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0cb012"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-localization",
      "expo-notifications"
    ]
  }
}
